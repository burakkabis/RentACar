    "FileLogConfiguration": {

      "FolderPath":"/logs/"
    },

    " MsSqlConfiguration": {
      "AutoCreateSqlTable": true,
      "ConncetionString": "Server=(localdb)\\mssqllocaldb; Database=RentACarDb;Trusted_Connection=true",
      "TableName": "Logs"
    }
  },
  "AllowedHosts": "*"
}
