{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "RentACar": "Server=(localdb)\\mssqllocaldb; Database=RentACarDb;Trusted_Connection=true"
  },

  "CacheSettings": {

    "SlidingExpiration": 2
  },
